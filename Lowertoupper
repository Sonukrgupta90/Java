import java.util.*;
class Lowertoupper
{
	public static void main(String args[])
	{
	    string str,srt1=" ";
	    char ch;
	    Scanner in=ner Scanner(System.in);
	    System.out.print("'enter a string");
	    str=in.next();
	    for(i=0;i<str.Length;i++)
	    {
	    ch=str.charAt(i);
	    str1+=ch.toLower();
	    }
	    System.out.print(str1);
	}
}